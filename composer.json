{
    "_readme": {
        "note": "THIS IS A DEVELOPMENT VERSION OF THIS PROJECT.",
        "TODO": "Remove the autoload section. Refactor the composer scripts away.",
    },
    "name": "drupal/drupal-project-legacy",
    "description": "Project template for Drupal 8",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "drupal/drupal-scaffold": "8.6.x-dev",
        "drupal/drupal-core-strict": "8.6.x-dev",
        "drupal/core": "8.6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "DrupalLegacyProject\\Core\\": "src/"
        }
    },
    "scripts": {
        "post-package-install": "DrupalLegacyProject\\Core\\Composer\\Composer::vendorTestCodeCleanup",
        "post-package-update": "DrupalLegacyProject\\Core\\Composer\\Composer::vendorTestCodeCleanup"
    }
}
